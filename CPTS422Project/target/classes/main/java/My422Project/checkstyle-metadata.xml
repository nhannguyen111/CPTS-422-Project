<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="My project checks"  priority="1600">
        <description>My project checks</description>

        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
            <quickfix classname="net.sf.eclipsecs.sample.checks.MethodLimitQuickfix" />
        </rule-metadata>
        
        <rule-metadata name="Halstead Length Check" internal-name="HalsteadLength" parent="TreeWalker">
            <alternative-name internal-name="My422Project.HalsteadLengthCheck" />
            <description>Halstead Length Check</description>
        </rule-metadata>
        
        <rule-metadata name="Num of Comments Check" internal-name="NumOfComments" parent="TreeWalker">
            <alternative-name internal-name="My422Project.NumOfCommentsCheck" />
            <description>Num of Comments Check</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead Vocabulary Check" internal-name="HalsteadVocabulary" parent="TreeWalker">
            <alternative-name internal-name="My422Project.HalsteadVocabularyCheck" />
            <description>Halstead Vocabulary Check</description>
        </rule-metadata>
        
        <rule-metadata name="Num Of Lines Of Comments Check" internal-name="NumOfLinesOfComments" parent="TreeWalker">
            <alternative-name internal-name="My422Project.NumOfLinesOfCommentsCheck" />
            <description>Num Of Lines Of Comments Check</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead Volume Check" internal-name="HalsteadVolume" parent="TreeWalker">
            <alternative-name internal-name="My422Project.HalsteadVolumeCheck" />
            <description>Halstead Volume Check</description>
        </rule-metadata>
        
        <rule-metadata name="Num Of Looping Statements Check" internal-name="NumOfLoopingStatements" parent="TreeWalker">
            <alternative-name internal-name="My422Project.NumOfLoopingStatementsCheck" />
            <description>Num Of Looping Statements Check</description>
        </rule-metadata>
        
    </rule-group-metadata>
</checkstyle-metadata>
